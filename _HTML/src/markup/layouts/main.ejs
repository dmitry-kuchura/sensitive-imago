		<%- partial('../views/widgets/head') %>
		<%- partial('../views/widgets/criticals') %>
	</head>
	<body>
		<% if (_activeFile.hasSeoText) { %>
			<main class="pageSeoText js-setSeoText">
				<div class="pageSize"><%- partial(_activeFile.seoTextContent, {}) %></div>
			</main>
			<!-- end pageSeoText -->
		<% } %>
		<div class="pageWrapper">
			<header class="pageHeader">
				<%- partial('../views/widgets/page-header', {}) %>
			</header>
			<!-- end pageHeader -->


			<div class="pageContainer">
				<%- body %>
			</div>
			<!-- end pageContainer -->


			<footer class="pageFooter">
				<%- partial('../views/widgets/page-footer', {}) %>
			</footer>
			<!-- end pageFooter -->

		</div>
		<!-- end pageWrapper -->




		<!-- внешние css файлы -->
